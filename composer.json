{
    "name": "quadstudio/laravel-rbac",
    "description": "RBAC for Laravel 5",
    "keywords": ["laravel", "rbac"],
    "homepage": "https://github.com/quadstudio/laravel-rbac",
    "type": "library",
    "license": "MIT",
    "version": "0.1.6",
    "authors": [
        {
            "name": "Alexander Shuleyko",
            "email": "salex009@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "QuadStudio\\Rbac\\": "src/Rbac/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "5.6.x"
    },
    "extra": {
        "laravel": {
            "providers": [
                "QuadStudio\\Rbac\\RbacServiceProvider"
            ],
            "aliases": {
                "Rbac": "QuadStudio\\Rbac\\Facades\\Rbac"
            }
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
